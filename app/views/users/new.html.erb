<div id="rubric">
  <div class="subtext">
    You can always click on <%= image_tag("guidance.png") %>
    for page-help.  
  </div>
</div>

<%= form_for(@user) do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  
  <%= render 'fields', :f => f %>
  <div class="actions">
    <%= f.submit "Sign up", :class => "create round" %>
  </div>
<% end %>

