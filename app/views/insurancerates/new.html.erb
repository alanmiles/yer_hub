<%= link_to image_tag("pageback.png", :title => "Insurance rules and rates"), insurancerule_path(@insurancerule), :class => "magnify" %>

<%= form_for([@country, @insurancerate]) do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  Rates for <%= @country.country.upcase %>
  <%= render 'fields', :f => f %>
  <div class="actions">
    <%= f.submit "Create Insurance Rates", :class => "create round" %>
  </div>
<% end %>
