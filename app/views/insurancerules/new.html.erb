<%= link_to image_tag("pageback.png", :title => "Insurance rules list"), insurancerules_path, :class => "magnify" %>

<%= form_for(@insurancerule) do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  
  <%= render 'fields', :f => f %>
  <div class="actions">
    <%= f.submit "Create Insurance Rule", :class => "create round" %>
  </div>
<% end %>
