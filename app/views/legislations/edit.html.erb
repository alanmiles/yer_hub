<%= link_to image_tag("pageback.png", :title => "Previous"), @legislation, :class => "magnify" %>

<%= form_for(@legislation) do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  <h1>Other rules for <%= @country.country %></h1>
  <%= render 'fields', :f => f %>
  
  <div class="actions">
    <%= f.submit "Update Country Rules", :class => "create round" %>
  </div>
<% end %>
